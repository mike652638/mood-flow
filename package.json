{
  "name": "trae-solo-builder-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "android:init": "npx cap add android",
    "android:sync": "npm run build && npx cap sync android",
    "android:build": "npm run android:release:prepare && npm run android:sync && npx cap build android && node scripts/prepare-release-clean-server-url.cjs --mode restore",
    "android:open": "npx cap open android",
    "android:run": "npx cap run android",
    "mobile:build": "npm run build && npx cap sync",
    "capacitor:sync": "npx cap sync",
    "capacitor:update": "npx cap update",
    "icons:optimize": "node scripts/optimize-icons.cjs",
    "icons:foreground": "node scripts/generate-foreground-icons.cjs --mode=normal",
    "icons:foreground:compact": "node scripts/generate-foreground-icons.cjs --mode=compact",
    "android:debug:auto": "cmd /c .\\build-android.cmd",
    "android:aab:auto": "cmd /c .\\build-android.cmd aab",
    "android:release:auto": "cmd /c .\\build-android.cmd release",
    "android:dev:wifi:auto": "node scripts/dev-android-wifi.cjs",
    "android:dev:usb:auto": "node scripts/dev-android-usb.cjs",
    "android:release:prepare": "node scripts/prepare-release-clean-server-url.cjs",
    "ci:push": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/ci-push-and-monitor.ps1",
    "ci:push:wait300": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/ci-push-and-monitor.ps1 -WaitSeconds 300"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/app": "^6.0.2",
    "@capacitor/browser": "^6.0.5",
    "@capacitor/camera": "^6.1.2",
    "@capacitor/cli": "^6.2.1",
    "@capacitor/core": "^6.2.1",
    "@capacitor/device": "^6.0.2",
    "@capacitor/filesystem": "^6.0.3",
    "@capacitor/keyboard": "^6.0.3",
    "@capacitor/local-notifications": "^6.1.2",
    "@capacitor/network": "^6.0.3",
    "@capacitor/share": "^6.0.3",
    "@capacitor/splash-screen": "^6.0.3",
    "@capacitor/status-bar": "^6.0.2",
    "@capacitor/toast": "^6.0.3",
    "@ionic/pwa-elements": "^3.3.0",
    "@langx/capacitor-voice-recorder": "^6.0.7",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-virtual": "^3.13.12",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/text": "^3.12.0",
    "@visx/wordcloud": "^3.12.0",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.511.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.3.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "sass": "^1.93.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
